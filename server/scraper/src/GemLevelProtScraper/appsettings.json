{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CacheProfiles": {
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  },
  "Compression": {
    "MimeTypes": [
      "application/vnd.restful+json",
      "application/problem+json",
      "image/x-icon"
    ]
  },
  "ForwardedHeaders": {
    "AllowedHosts": ["example.com"],
    "ForwardedHeaders": "XForwardedFor,XForwardedHost,XForwardedProto"
  },
  "Host": {
    "ShutdownTimeout": "00:03:00"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 10240,
      "MaxRequestHeaderCount": 40
    }
  },
  "Database": {
    "PoeNinjaDatabaseSettings": {
      "DatabaseName": "PoeNinja",
      "GemPriceCollectionName": "GemPricing"
    },
    "PoeDbDatabaseSettings": {
      "DatabaseName": "PoeDb",
      "SkillCollectionName": "PoeDbSkill"
    }
  },
  "Authentication": {
    "ApiKey": "test"
  }
}
