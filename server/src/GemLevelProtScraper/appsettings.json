{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CacheProfiles": {
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  },
  "Compression": {
    "MimeTypes": [
      "application/vnd.restful+json",
      "application/problem+json",
      "image/x-icon"
    ]
  },
  "ForwardedHeaders": {
    "AllowedHosts": [
      "example.com"
    ],
    "ForwardedHeaders": "XForwardedFor,XForwardedHost,XForwardedProto"
  },
  "Host": {
    "ShutdownTimeout": "00:03:00"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 10240,
      "MaxRequestHeaderCount": 40
    }
  },
  "Database": {
    "PoeDatabaseSettings": {
      "DatabaseName": "Poe",
      "PoeLeagueCollectionName": "PoeLeague",
      "PoeDbSkillCollectionName": "PoeDbSkill",
      "PoeNinjaGemCollectionName": "PoeNinjaGem",
      "PoeNinjaCurrencyCollectionName": "PoeNinjaCurrency",
      "SkillGemViewName": "SkillGem"
    }
  },
  "ProfitService": {
    "SpecialExperienceFactorPerQualityGams": {
      "Empower Support": 0.05,
      "Awakened Empower Support": 0.05,
      "Enhance Support": 0.05,
      "Awakened Enhance  Support": 0.05,
      "Enlighten Support": 0.05,
      "Awakened Enlighten Support": 0.05
    }
  }
}
